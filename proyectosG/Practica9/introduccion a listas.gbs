function secuenciaAritmeticaDeNumeros(desde, hasta){
    listaCreada := []
    nroActual := desde
    while (nroActual <= hasta){
        listaCreada := listaCreada ++ [nroActual]
        nroActual := nroActual + 1
    }
   
    return(listaCreada)
}

function enTotal_IgualesA_(cantidad, elemento){
    listaCreada := []
    elementoActual := elemento
    repeat (cantidad){
        listaCreada := listaCreada ++ [elemento]
    }
    return(listaCreada)
}

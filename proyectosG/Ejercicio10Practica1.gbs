program{
    DibujarCuadradoMulticolorDeLado4()
}
procedure PonerUnaDeCada(){
    Poner(Azul);
    Poner(Rojo);
    Poner(Negro);
    Poner(Verde);
}
procedure Mover3AlOeste(){
    Mover(Oeste);
    Mover(Oeste);
    Mover(Oeste);
}
procedure DibujarLíneaMulticolorDeLargo4(){
    PonerUnaDeCada();
    Mover(Este);
    PonerUnaDeCada();
    Mover(Este);
    PonerUnaDeCada();
    Mover(Este);
    PonerUnaDeCada();
    Mover3AlOeste()
    
}
procedure DibujarCuadradoMulticolorDeLado4(){
    DibujarLíneaMulticolorDeLargo4()
    Mover(Norte);
    PonerUnaDeCada();
    Mover(Norte);
    PonerUnaDeCada();
    Mover(Norte);
    DibujarLíneaMulticolorDeLargo4();
    Mover3AlEste();
    Mover(Sur);
    PonerUnaDeCada();
    Mover(Sur);
    PonerUnaDeCada();
    Mover(Sur);
    Mover3AlOeste();
    
    
}
procedure Mover3AlEste(){
    Mover(Este);
    Mover(Este);
    Mover(Este);
    
}

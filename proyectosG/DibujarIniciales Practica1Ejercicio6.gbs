}
procedure DibujarLRoja(){
    Poner(Rojo);
    Mover(Norte);
    Poner(Rojo);
    Mover(Norte);
    Poner(Rojo);
    Mover(Sur);
    Mover(Sur);
    Mover(Este);
    Poner(Rojo);
    Mover(Este);
    Poner(Rojo)
}
procedure VolverAlInicio(){
    IrAlBorde(Oeste);
    IrAlBorde(Sur);
}